{
  "User": {
    "tablename": "user",
    "columns": {
      "id": "Integer, primary_key=True, autoincrement=True",
      "name": "String(100), nullable=False",
      "created_at": "DateTime, default=datetime.utcnow"
    },
    "relationships": {
      "files": {
        "model": "FileData",
        "backref": "user"
      },
      "invoices": {
        "model": "Invoice",
        "backref": "user"
      }
    }
  },

  "FileData": {
    "tablename": "file_data",
    "columns": {
      "id": "Integer, primary_key=True, autoincrement=True",
      "user_id": "Integer, ForeignKey('user.id'), nullable=False",
      "filename": "String(255), nullable=False",
      "file_path": "String(500), nullable=False",
      "file_type": "String(200), nullable=False",
      "created_at": "DateTime, default=datetime.utcnow",
      "updated_at": "DateTime, default=datetime.utcnow"
    },
    "relationship": {
      "invoices": {
        "model": "Invoice",
        "backref": "file_data"
      }
    }
  },

  "Invoice": {
    "tablename": "invoice",
    "columns": {
      "id": "Integer, primary_key=True, autoincrement=True",
      "user_id": "Integer, ForeignKey('user.id'), nullable=False",
      "file_data_id": "Integer, ForeignKey('file_data.id'), nullable=False",
      "company": "String(200), nullable=False",
      "invoice_number": "String(200), nullable=False, unique=True",
      "due_date": "DateTime, nullable=False",
      "issue_date": "DateTime",
      "amount": "Float, nullable=False",
      "currency": "String(20), nullable=False, default='EUR'",
      "description": "String(1000)",
      "created_at": "DateTime, default=datetime.utcnow",
      "updated_at": "DateTime, default=datetime.utcnow"
    }
  }
}
